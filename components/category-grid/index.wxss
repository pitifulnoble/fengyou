.container {
    width: 100%;
    height: 360rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #ffffff;
}

.inner-container {
    width: 730rpx;
    height: 340rpx;
    border: 1px dashed #DCEBE6;
}

.grid-item {
    width: 150rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.img {
    width: 60rpx;
    height: 60rpx;
}

.text {
    margin-top: 6rpx;
    font-size: 24rpx;
    color: #333333;
}
